                                        </div>
                                        <!--end::Menu item-->
                                        <!--begin::Menu item-->
                                        <div class="menu-item">
                                            <!--begin::Menu link-->
                                            <a class="menu-link nav-link py-3 px-4 px-xxl-6" href="#pricing" data-kt-scroll-toggle="true" data-kt-drawer-dismiss="true">Pricing</a>
                                            <!--end::Menu link-->
                                        </div>
                                        <!--end::Menu item-->
                                    </div>
                                    <!--end::Menu-->
                                </div>
                            </div>
                            <!--end::Menu wrapper-->

                            @if (signInManager.IsSignedIn(User))
                            {
                                <div class="flex-equal text-end ms-1">
                                    <a asp-controller="Dashboard" asp-action="Index" class="btn btn-success">Dashboard </a>
                                </div>

                                
                                <div class="flex-equal text-end ms-1">
                                    <a asp-controller="Account" asp-action="LogOut" class="btn btn-success">Log Out</a>
                                </div>


                            }
                            else
                            {
                                <div class="flex-equal text-end ms-1">
                                    <a asp-controller="Account" asp-action="Login" class="btn btn-success">Sign In</a>
                                </div>
                            }

                        </div>
                        <!--end::Wrapper-->
                    </div>
                    <!--end::Container-->
                </div>
                <!--end::Header-->
                <!--begin::Landing hero-->
